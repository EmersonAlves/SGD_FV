<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sgdfv.emerson.sgd_fv">

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/fvicon"
        android:label="FV Comercial"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".MainSplash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainOrcamento"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainUsuario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityItem"
            android:screenOrientation="portrait" />
        <activity android:name=".ActivityHistorico"></activity>
    </application>

</manifest>